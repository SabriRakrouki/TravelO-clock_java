            <Label prefHeight="21.0" prefWidth="89.0" text="Nom">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="136.0" layoutY="71.0" prefHeight="25.0" prefWidth="194.0">
         <children>
            <TextField fx:id="txtName" />
         </children>
      </VBox>
      <TableView fx:id="tabView" layoutX="400.0" layoutY="84.0" prefHeight="234.0" prefWidth="309.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="colName" prefWidth="233.0" text="Nom" />
        </columns>
      </TableView>
      <Button fx:id="btnInsert" layoutX="112.0" layoutY="250.0" mnemonicParsing="false" onAction="#handelInsertAction" text="Insert " />
      <Button fx:id="btndelete" layoutX="182.0" layoutY="250.0" mnemonicParsing="false" onAction="#handelDeleteAction" text="Delete" />
